{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".env*",
    "tsconfig.build.json"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**",
        "build/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": []
    },
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "build"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": []
    },
    "clean": {
      "cache": false
    }
  }
}